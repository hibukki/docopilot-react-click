{
  "name": "docopilot-react-click",
  "version": "1.0.0",
  "type": "module",
  "description": "Google Docs sidebar add-on that generates AI comments using Gemini",
  "repository": {
    "type": "git",
    "url": "https://github.com/hibukki/docopilot-react-click.git"
  },
  "scripts": {
    "dev": "vite",
    "lint": "eslint .",
    "login": "clasp login",
    "setup": "rm -f .clasp.json && mkdir -p dist && clasp create --type docs --title \"Docopilot\" --rootDir ./dist && mv ./dist/.clasp.json ./.clasp.json && rm -rf dist",
    "open": "clasp open --addon",
    "push": "clasp push",
    "setup:https": "mkdirp certs && mkcert -key-file ./certs/key.pem -cert-file ./certs/cert.pem localhost 127.0.0.1",
    "build:dev": "tsc && vite build --mode development",
    "build": "tsc && vite build --mode production",
    "deploy:dev": "yarn build:dev && yarn push",
    "deploy": "yarn build && yarn push",
    "deploy:watch": "npx nodemon --watch src -e js,jsx,ts,tsx,html,css --exec 'npm run deploy'",
    "start": "yarn deploy:dev && yarn dev"
  },
  "keywords": [
    "react",
    "google",
    "apps",
    "script",
    "docs"
  ],
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/material": "^5.11.11",
    "gas-client": "^1.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@google/clasp": "^2.4.2",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-googleappsscript": "^1.0.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "gas-types-detailed": "^1.1.2",
    "prettier": "^2.7.0",
    "rollup": "^4.18.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-singlefile": "^2.0.1",
    "vite-plugin-static-copy": "^1.0.1"
  }
}
